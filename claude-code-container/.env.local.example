# Local Development Environment Variables
# Copy this to .env.local and update values

# AWS Configuration
NODE_ENV=development
AWS_PROFILE=personal
AWS_REGION=us-west-2
AWS_ACCOUNT_ID=942734823970

# S3 Bucket Configuration
# Note: S3 bucket comes from message.projectId now
S3_BUCKET_PREFIX=edit  # Bucket pattern: {prefix}.{projectId}.webordinary.com

# GitHub Configuration
GITHUB_TOKEN=your_github_token_here

# Workspace Configuration
WORKSPACE_PATH=/workspace

# Claude Configuration via AWS Bedrock
# Note: No ANTHROPIC_API_KEY needed when using Bedrock
# Authentication uses AWS credentials (AWS_PROFILE or IAM role)

# Bedrock Configuration for Claude Code SDK
CLAUDE_CODE_USE_BEDROCK=1
ANTHROPIC_MODEL=us.anthropic.claude-3-7-sonnet-20250219-v1:0
ANTHROPIC_SMALL_FAST_MODEL=us.anthropic.claude-3-5-haiku-20241022-v1:0
CLAUDE_CODE_MAX_OUTPUT_TOKENS=4096
MAX_THINKING_TOKENS=1024
# Note: AWS_REGION already set above to us-west-2

# Simulation mode (set to false to use real Bedrock)
CLAUDE_SIMULATION_MODE=true

# Logging
LOG_LEVEL=debug

# Queue Configuration
UNCLAIMED_QUEUE_URL=https://sqs.us-west-2.amazonaws.com/942734823970/webordinary-unclaimed
OWNERSHIP_TABLE_NAME=webordinary-container-ownership

# REMOVED Legacy Variables:
# CLIENT_ID - comes from message.projectId
# DEFAULT_CLIENT_ID - no defaults, dynamic claiming
# DEFAULT_USER_ID - no defaults, dynamic claiming
# AUTO_SHUTDOWN_MINUTES - containers stay warm
# REPO_URL - comes from message.repoUrl